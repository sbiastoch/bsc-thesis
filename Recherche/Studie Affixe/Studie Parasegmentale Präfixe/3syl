SELECT COUNT( * ) , SUBSTR( word, 1, 3 ) , COUNT( DISTINCT strsclass ) , strsclass
FROM  `lemmas2` 
WHERE mysylcnt =3
GROUP BY SUBSTR( word, 1, 3 ) 
HAVING COUNT( * ) >20
AND COUNT( DISTINCT strsclass ) =1
ORDER BY COUNT( * ) DESC ,  `lemmas2`.`StrsClass` ASC

